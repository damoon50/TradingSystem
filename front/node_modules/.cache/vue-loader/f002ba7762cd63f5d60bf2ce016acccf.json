v-else>\n                        <el-form-item v-if=\"ruleForm.shangpinPhoto\" label=\"商品照片\" prop=\"shangpinPhoto\">\n                            <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in (ruleForm.shangpinPhoto || '').split(',')\" :src=\"item\" width=\"100\" height=\"100\">\n                        </el-form-item>\n                    </div>\n                </el-col>\n                <el-col :span=\"12\">\n                    <el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"游戏类型\" prop=\"shangpinTypes\">\n                        <el-select v-model=\"ruleForm.shangpinTypes\" :disabled=\"ro.shangpinTypes\" placeholder=\"请选择游戏类型\">\n                            <el-option\n                                v-for=\"(item,index) in shangpinTypesOptions\"\n                                v-bind:key=\"item.codeIndex\"\n                                :label=\"item.indexName\"\n                                :value=\"item.codeIndex\">\n                            </el-option>\n                        </el-select>\n                    </el-form-item>\n                    <div v-else-if=\"type=='info'\">\n                        <el-form-item class=\"input\" label=\"游戏类型\" prop=\"shangpinValue\">\n                        <el-input v-model=\"ruleForm.shangpinValue\"\n                            placeholder=\"游戏类型\" readonly></el-input>\n                        </el-form-item>\n                    </div>\n                </el-col>\n                <el-col :span=\"12\">\n                    <el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"游戏玩法\" prop=\"youxiwanfaTypes\">\n                        <el-select v-model=\"ruleForm.youxiwanfaTypes\" :disabled=\"ro.youxiwanfaTypes\" placeholder=\"请选择游戏玩法\">\n                            <el-option\n                                v-for=\"(item,index) in youxiwanfaTypesOptions\"\n                                v-bind:key=\"item.codeIndex\"\n                                :label=\"item.indexName\"\n                                :value=\"item.codeIndex\">\n                            </el-option>\n                        </el-select>\n                    </el-form-item>\n                    <div v-else-if=\"type=='info'\">\n                        <el-form-item class=\"input\" label=\"游戏玩法\" prop=\"youxiwanfaValue\">\n                        <el-input v-model=\"ruleForm.youxiwanfaValue\"\n                            placeholder=\"游戏玩法\" readonly></el-input>\n                        </el-form-item>\n                    </div>\n                </el-col>\n                <el-col :span=\"12\">\n                    <el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"交易类型\" prop=\"jiaoyileixingTypes\">\n                        <el-select v-model=\"ruleForm.jiaoyileixingTypes\" :disabled=\"ro.jiaoyileixingTypes\" placeholder=\"请选择交易类型\">\n                            <el-option\n                                v-for=\"(item,index) in jiaoyileixingTypesOptions\"\n                                v-bind:key=\"item.codeIndex\"\n                                :label=\"item.indexName\"\n                                :value=\"item.codeIndex\">\n                            </el-option>\n                        </el-select>\n                    </el-form-item>\n                    <div v-else-if=\"type=='info'\">\n                        <el-form-item class=\"input\" label=\"交易类型\" prop=\"jiaoyileixingValue\">\n                        <el-input v-model=\"ruleForm.jiaoyileixingValue\"\n                            placeholder=\"交易类型\" readonly></el-input>\n                        </el-form-item>\n                    </div>\n                </el-col>\n                <el-col :span=\"12\">\n                    <el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"操作方式\" prop=\"caozuofangshiTypes\">\n                        <el-select v-model=\"ruleForm.caozuofangshiTypes\" :disabled=\"ro.caozuofangshiTypes\" placeholder=\"请选择操作方式\">\n                            <el-option\n                                v-for=\"(item,index) in caozuofangshiTypesOptions\"\n                                v-bind:key=\"item.codeIndex\"\n                                :label=\"item.indexName\"\n                                :value=\"item.codeIndex\">\n                            </el-option>\n                        </el-select>\n                    </el-form-item>\n                    <div v-else-if=\"type=='info'\">\n                        <el-form-item class=\"input\" label=\"操作方式\" prop=\"caozuofangshiValue\">\n                        <el-input v-model=\"ruleForm.caozuofangshiValue\"\n                            placeholder=\"操作方式\" readonly></el-input>\n                        </el-form-item>\n                    </div>\n                </el-col>\n                <el-col :span=\"12\">\n                    <el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"是否热门\" prop=\"remenTypes\">\n                        <el-select v-model=\"ruleForm.remenTypes\" :disabled=\"ro.remenTypes\" placeholder=\"请选择是否热门\">\n                            <el-option\n                                v-for=\"(item,index) in remenTypesOptions\"\n                                v-bind:key=\"item.codeIndex\"\n                                :label=\"item.indexName\"\n                                :value=\"item.codeIndex\">\n                            </el-option>\n                        </el-select>\n                    </el-form-item>\n                    <div v-else-if=\"type=='info'\">\n                        <el-form-item class=\"input\" label=\"是否热门\" prop=\"remenValue\">\n                        <el-input v-model=\"ruleForm.remenValue\"\n                            placeholder=\"是否热门\" readonly></el-input>\n                        </el-form-item>\n                    </div>\n                </el-col>\n               <el-col :span=\"12\">\n                   <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"商品库存\" prop=\"shangpinKucunNumber\">\n                       <el-input v-model=\"ruleForm.shangpinKucunNumber\"\n                                 placeholder=\"商品库存\" clearable  :readonly=\"ro.shangpinKucunNumber\"></el-input>\n                   </el-form-item>\n                   <div v-else-if=\"type=='info'\">\n                       <el-form-item class=\"input\" label=\"商品库存\" prop=\"shangpinKucunNumber\">\n                           <el-input v-model=\"ruleForm.shangpinKucunNumber\"\n                                     placeholder=\"商品库存\" readonly></el-input>\n                       </el-form-item>\n                   </div>\n               </el-col>\n               <el-col :span=\"12\">\n                   <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"购买获得积分\" prop=\"shangpinPrice\">\n                       <el-input v-model=\"ruleForm.shangpinPrice\"\n                                 placeholder=\"购买获得积分\" clearable  :readonly=\"ro.shangpinPrice\"></el-input>\n                   </el-form-item>\n                   <div v-else-if=\"type=='info'\">\n                       <el-form-item class=\"input\" label=\"购买获得积分\" prop=\"shangpinPrice\">\n                           <el-input v-model=\"ruleForm.shangpinPrice\"\n                                     placeholder=\"购买获得积分\" readonly></el-input>\n                       </el-form-item>\n                   </div>\n               </el-col>\n               <el-col :span=\"12\">\n                   <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"商品原价\" prop=\"shangpinOldMoney\">\n                       <el-input v-model=\"ruleForm.shangpinOldMoney\"\n                                 placeholder=\"商品原价\" clearable  :readonly=\"ro.shangpinOldMoney\"></el-input>\n                   </el-form-item>\n                   <div v-else-if=\"type=='info'\">\n                       <el-form-item class=\"input\" label=\"商品原价\" prop=\"shangpinOldMoney\">\n                           <el-input v-model=\"ruleForm.shangpinOldMoney\"\n                                     placeholder=\"商品原价\" readonly></el-input>\n                       </el-form-item>\n                   </div>\n               </el-col>\n               <el-col :span=\"12\">\n                   <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"现价/积分\" prop=\"shangpinNewMoney\">\n                       <el-input v-model=\"ruleForm.shangpinNewMoney\"\n                                 placeholder=\"现价/积分\" clearable  :readonly=\"ro.shangpinNewMoney\"></el-input>\n                   </el-form-item>\n                   <div v-else-if=\"type=='info'\">\n                       <el-form-item class=\"input\" label=\"现价/积分\" prop=\"shangpinNewMoney\">\n                           <el-input v-model=\"ruleForm.shangpinNewMoney\"\n                                     placeholder=\"现价/积分\" readonly></el-input>\n                       </el-form-item>\n                   </div>\n               </el-col>\n                <el-col :span=\"24\">\n                    <el-form-item v-if=\"type!='info' && !ro.shangpinContent\"  label=\"商品介绍\" prop=\"shangpinContent\">\n                        <editor style=\"min-width: 200px; max-width: 600px;\"\n                                v-model=\"ruleForm.shangpinContent\"\n                                class=\"editor\"\n                                action=\"file/upload\">\n                        </editor>\n                    </el-form-item>\n                    <div v-else-if=\"type=='info'\">\n                        <el-form-item v-if=\"ruleForm.shangpinContent\" label=\"商品介绍\" prop=\"shangpinContent\">\n                            <span v-html=\"ruleForm.shangpinContent\"></span>\n                        </el-form-item>\n                    </div>\n                </el-col>\n            </el-row>\n            <el-form-item class=\"btn\">\n                <el-button v-if=\"type!='info'\" type=\"primary\" class=\"btn-success\" @click=\"onSubmit\">提交</el-button>\n                <el-button v-if=\"type!='info'\" class=\"btn-close\" @click=\"back()\">取消</el-button>\n                <el-button v-if=\"type=='info'\" class=\"btn-close\" @click=\"back()\">返回</el-button>\n            </el-form-item>\n        </el-form>\n    </div>\n</template>\n<script>\n    import styleJs from \"../../../utils/style.js\";\n    // 数字，邮件，手机，url，身份证校验\n    import { isNumber,isIntNumer,isEmail,isPhone, isMobile,isURL,checkIdCard } from \"@/utils/validate\";\n    export default {\n        data() {\n            return {\n                addEditForm:null,\n                id: '',\n                type: '',\n                sessionTable : \"\",//登录账户所在表名\n                role : \"\",//权限\n                userId:\"\",//当前登录人的id\n                ro:{\n                    shangpinName: false,\n                    shangpinUuidNumber: false,\n                    shangpinPhoto: false,\n                    shangpinTypes: false,\n                    youxiwanfaTypes: false,\n                    jiaoyileixingTypes: false,\n                    caozuofangshiTypes: false,\n                    remenTypes: false,\n                    shangpinKucunNumber: false,\n                    shangpinPrice: false,\n                    shangpinOldMoney: false,\n                    shangpinNewMoney: false,\n                    shangpinClicknum: false,\n                    shangpinContent: false,\n                    shangxiaTypes: false,\n                    shangpinDelete: false,\n                },\n                ruleForm: {\n                    shangpinName: '',\n                    shangpinUuidNumber: new Date().getTime(),\n                    shangpinPhoto: '',\n                    shangpinTypes: '',\n                    youxiwanfaTypes: '',\n                    jiaoyileixingTypes: '',\n                    caozuofangshiTypes: '',\n                    remenTypes: '',\n                    shangpinKucunNumber: '',\n                    shangpinPrice: '',\n                    shangpinOldMoney: '',\n                    shangpinNewMoney: '',\n                    shangpinClicknum: '',\n                    shangpinContent: '',\n                    shangxiaTypes: '',\n                    shangpinDelete: '',\n                },\n                shangpinTypesOptions : [],\n                youxiwanfaTypesOptions : [],\n                jiaoyileixingTypesOptions : [],\n                caozuofangshiTypesOptions : [],\n                remenTypesOptions : [],\n                shangxiaTypesOptions : [],\n                rules: {\n                   shangpinName: [\n                              { required: true, message: '商品名称不能为空', trigger: 'blur' },\n                          ],\n                   shangpinUuidNumber: [\n                              { required: true, message: '商品编号不能为空', trigger: 'blur' },\n                          ],\n                   shangpinPhoto: [\n                              { required: true, message: '商品照片不能为空', trigger: 'blur' },\n                          ],\n                   shangpinTypes: [\n                              { required: true, message: '游戏类型不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   youxiwanfaTypes: [\n                              { required: true, message: '游戏玩法不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   jiaoyileixingTypes: [\n                              { required: true, message: '交易类型不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   caozuofangshiTypes: [\n                              { required: true, message: '操作方式不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   remenTypes: [\n                              { required: true, message: '是否热门不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   shangpinKucunNumber: [\n                              { required: true, message: '商品库存不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   shangpinPrice: [\n                              { required: true, message: '购买获得积分不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   shangpinOldMoney: [\n                              { required: true, message: '商品原价不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]{0,6}(\\.[0-9]{1,2})?$/,\n                                  message: '只允许输入整数6位,小数2位的数字',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   shangpinNewMoney: [\n                              { required: true, message: '现价/积分不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]{0,6}(\\.[0-9]{1,2})?$/,\n                                  message: '只允许输入整数6位,小数2位的数字',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   shangpinClicknum: [\n                              { required: true, message: '商品热度不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   shangpinContent: [\n                              { required: true, message: '商品介绍不能为空', trigger: 'blur' },\n                          ],\n                   shangxiaTypes: [\n                              { required: true, message: '是否上架不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                   shangpinDelete: [\n                              { required: true, message: '逻辑删除不能为空', trigger: 'blur' },\n                              {  pattern: /^[0-9]*$/,\n                                  message: '只允许输入整数',\n                                  trigger: 'blur'\n                              }\n                          ],\n                }\n            };\n        },\n        props: [\"parent\"],\n        computed: {\n        },\n        created() {\n            //获取当前登录用户的信息\n            this.sessionTable = this.$storage.get(\"sessionTable\");\n            this.role = this.$storage.get(\"role\");\n            this.userId = this.$storage.get(\"userId\");\n\n\n            if (this.role != \"管理员\"){\n            }\n            this.addEditForm = styleJs.addStyle();\n            this.addEditStyleChange()\n            this.addEditUploadStyleChange()\n            //获取下拉框信息\n                this.$http({\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=shangpin_types`,\n                    method: \"get\"\n                }).then(({ data }) => {\n                    if (data && data.code === 0) {\n                        this.shangpinTypesOptions = data.data.list;\n                    }\n                });\n                this.$http({\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=youxiwanfa_types`,\n                    method: \"get\"\n                }).then(({ data }) => {\n                    if (data && data.code === 0) {\n                        this.youxiwanfaTypesOptions = data.data.list;\n                    }\n                });\n                this.$http({\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=jiaoyileixing_types`,\n                    method: \"get\"\n                }).then(({ data }) => {\n                    if (data && data.code === 0) {\n                        this.jiaoyileixingTypesOptions = data.data.list;\n                    }\n                });\n                this.$http({\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=caozuofangshi_types`,\n                    method: \"get\"\n                }).then(({ data }) => {\n                    if (data && data.code === 0) {\n                        this.caozuofangshiTypesOptions = data.data.list;\n                    }\n                });\n                this.$http({\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=remen_types`,\n                    method: \"get\"\n                }).then(({ data }) => {\n                    if (data && data.code === 0) {\n                        this.remenTypesOptions = data.data.list;\n                    }\n                });\n                this.$http({\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=shangxia_types`,\n                    method: \"get\"\n                }).then(({ data }) => {\n                    if (data && data.code === 0) {\n                        this.shangxiaTypesOptions = data.data.list;\n                    }\n                });\n\n\n        },\n        mounted() {\n        },\n        methods: {\n            // 下载\n            download(file){\n                window.open(`${file}`)\n            },\n            // 初始化\n            init(id,type) {\n                if (id) {\n                    this.id = id;\n                    this.type = type;\n                }\n                if(this.type=='info'||this.type=='else'){\n                    this.info(id);\n                }\n                // 获取用户信息\n                this.$http({\n                    url:`${this.$storage.get(\"sessionTable\")}/session`,\n                    method: \"get\"\n                }).then(({ data }) => {\n                    if (data && data.code === 0) {\n                        var json = data.data;\n                    } else {\n                        this.$message.error(data.msg);\n                    }\n                });\n            },\n            // 多级联动参数\n            info(id) {\n                let _this =this;\n                _this.$http({\n                    url: `shangpin/info/${id}`,\n                    method: 'get'\n                }).then(({ data }) => {\n                    if (data && data.code === 0) {\n                        _this.ruleForm = data.data;\n                    } else {\n                        _this.$message.error(data.msg);\n                    }\n                });\n            },\n            // 提交\n            onSubmit() {\n                this.$refs[\"ruleForm\"].validate(valid => {\n                    if (valid) {\n                        this.$http({\n                            url:`shangpin/${!this.ruleForm.id ? \"save\" : \"update\"}`,\n                            method: \"post\",\n                            data: this.ruleForm\n                        }).then(({ data }) => {\n                            if (data && data.code === 0) {\n                                this.$message({\n                                    message: \"操作成功\",\n                                    type: \"success\",\n                                    duration: 1500,\n                                    onClose: () => {\n                                        this.parent.showFlag = true;\n                                        this.parent.addOrUpdateFlag = false;\n                                        this.parent.shangpinCrossAddOrUpdateFlag = false;\n                                        this.parent.search();\n                                        this.parent.contentStyleChange();\n                                    }\n                                });\n                            } else {\n                                this.$message.error(data.msg);\n                            }\n                        });\n                    }\n                });\n            },\n            // 获取uuid\n            getUUID () {\n                return new Date().getTime();\n            },\n            // 返回\n            back() {\n                this.parent.showFlag = true;\n                this.parent.addOrUpdateFlag = false;\n                this.parent.shangpinCrossAddOrUpdateFlag = false;\n                this.parent.contentStyleChange();\n            },\n            //图片\n            shangpinPhotoUploadChange(fileUrls){\n                this.ruleForm.shangpinPhoto = fileUrls;\n                this.addEditUploadStyleChange()\n            },\n\n            addEditStyleChange() {\n                this.$nextTick(()=>{\n                    // input\n                    document.querySelectorAll('.addEdit-block .input .el-input__inner').forEach(el=>{\n                        el.style.height = this.addEditForm.inputHeight\n                        el.style.color = this.addEditForm.input